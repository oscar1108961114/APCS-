import java.util.Scanner;
public class o19
{
	public static void main(String[] args)
	{
		Scanner cin = new Scanner(System.in);
		int n = cin.nextInt();
		int result = 0;
		int max1 = Integer.MIN_VALUE;
		int max2 = Integer.MIN_VALUE;
		int ai1 = 0,di1 = 0;
		int ai2 = 0,di2 = 0;
		for(int i=0;i<n;i++)
		{
			int ai = cin.nextInt();
			int di = cin.nextInt();
			int stats = ai*ai + di*di;
			if(stats > max1)
			{
				max2 = max1;
				ai2 = ai1;
				di2 = di1;
				max1 = stats;
				ai1 = ai;
				di1 = di;				
			}
			else if(stats > max2)
			{
				max2 = stats;
				ai2 = ai;
		        di2 = di;
			}
			else
			{
			   continue;
			}
		}
		System.out.println(ai2 + " " + di2);
	}
}
