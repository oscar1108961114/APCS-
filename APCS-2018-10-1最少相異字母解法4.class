import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.HashSet;
public class o00005
{
    public static int a(String str)
    {
       HashSet<Character> letter = new HashSet<Character>();
       for(int i=0;i<str.length();i++)
       {
    	  letter.add(str.charAt(i));
       }
       return letter.size();
    }
	public static void main(String[] args)
	{
		try(InputStreamReader cin = new InputStreamReader(System.in);
			BufferedReader br = new BufferedReader(cin))
		{
			int row = Integer.parseInt(br.readLine());
			int mincount = Integer.MAX_VALUE;
			String result = "";
			for(int i=0;i<row;i++)
			{
			   String data = br.readLine();
			   int count1  = a(data);
			   if(count1<mincount ||
			     (count1== mincount&& data.compareTo(result) < 0))
			   {
			     mincount = count1;
			     result = data;
			   }
			}
			System.out.println(result);
		 }
		 catch (Exception e)
		 {
		   e.printStackTrace();
		 }

	}

}
